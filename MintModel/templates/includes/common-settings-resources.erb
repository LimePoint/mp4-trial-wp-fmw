            <% if defined? @database %>
            ,
            {
                "name": "JdbcCommonSettings",
                "type": "JDBCSystemResource",
                "params": [
                    {
                        "type": "JDBCResource",
                        "params": [
                            {
                                "type": "JDBCOracleParams",
                                "attributes": {
                                "OnsNodeList": "<%= @database['ons_node_list'] %>",
                                "FanEnabled": "true"
                                }
                            },
                            {
                                "type": "JDBCConnectionPoolParams",
                                "attributes": {
                                    "MinCapacity": "<%= @common['jdbc']['min_capacity'] %>",
                                    "TestConnectionsOnReserve": "true",
                                    <% if ['obpdoc','obpipm','obpoam','obpidm','obpoim','obpcim','obpurm'].include?@asset_code %>
                                    "TestTableName": "SQL SELECT 1 FROM DUAL"
                                    <% else %>
                                    "TestTableName": "SQL ISVALID"
                                    <% end %>,
                                    <% if ['obpobh','obposb'].include?@asset_code %>
                                    "WrapTypes": "false"
                                    <% end %>
                                }
                            },
                            {
                                "type": "JDBCDriverParams",
                                "attributes": {
                                <% if @set_dual_site_url == "true" %>
                                    <% if @enable_dbssl == "true" %>
                                        <% if @asset_code == 'obpobh' or @asset_code == 'obpbat' %>
                                            "Url": "<%= @database['sysconfig_url_ssl'] %>"
                                        <% elsif @asset_code == 'obpobu' %>
                                            "Url": "<%= @database['ui_db_url_ssl'] %>"
                                        <% elsif @asset_code == 'obpdoc' %>
                                            "Url": "<%= @database['doc_db_url_ssl'] %>"
                                        <% else %>
                                            "Url": "jdbc:oracle:thin:@(DESCRIPTION_LIST=(LOAD_BALANCE=OFF)(FAILOVER=ON)(DESCRIPTION=(ADDRESS=(PROTOCOL=TCPS)(HOST=<%= @database['scan_address'] %>)(PORT=<%= @database['listen_port_ssl'] %>))(CONNECT_DATA=(SERVICE_NAME=<%= @database['service_name'] %>))(SECURITY=(SSL_SERVER_CERT_DN=<%= @common['jdbc']['ssl_dn'] %>)))(DESCRIPTION=(ADDRESS=(PROTOCOL=TCPS)(HOST=<%= @database['secondary_scan_address'] %>)(PORT=<%= @database['listen_port_ssl'] %>))(CONNECT_DATA=(SERVICE_NAME=<%= @database['service_name'] %>))(SECURITY=(SSL_SERVER_CERT_DN=<%= @common['jdbc']['secondary_ssl_dn'] %>))))"
                                        <% end %>
                                    <% else %>
                                        "Url": "jdbc:oracle:thin:@(DESCRIPTION_LIST=(LOAD_BALANCE=OFF)(FAILOVER=ON)(DESCRIPTION=(ADDRESS=(PROTOCOL=TCP)(HOST=<%= @database['scan_address'] %>)(PORT=<%= @database['listen_port'] %>))(CONNECT_DATA=(SERVICE_NAME=<%= @database['service_name'] %>)))(DESCRIPTION=(ADDRESS=(PROTOCOL=TCP)(HOST=<%= @database['secondary_scan_address'] %>)(PORT=<%= @database['listen_port'] %>))(CONNECT_DATA=(SERVICE_NAME=<%= @database['service_name'] %>))))"
                                    <% end %>
                                <% else %>
                                    <% if @enable_dbssl == "true" %>
                                        "Url": "jdbc:oracle:thin:@(DESCRIPTION=(ADDRESS=(PROTOCOL=TCPS)(HOST=<%= @database['scan_address'] %>)(PORT=<%= @database['listen_port_ssl'] %>))(CONNECT_DATA=(SERVICE_NAME=<%= @database['service_name'] %>))(SECURITY=(SSL_SERVER_CERT_DN=<%= @common['jdbc']['ssl_dn'] %>)))"
                                    <% else %>
                                        "Url": "jdbc:oracle:thin:@(DESCRIPTION=(ADDRESS=(PROTOCOL=TCP)(HOST=<%= @database['scan_address'] %>)(PORT=<%= @database['listen_port'] %>))(CONNECT_DATA=(SERVICE_NAME=<%= @database['service_name'] %>)))"
                                    <% end %>
                                <% end %>
                                },
                                "properties": {
                                    "javax.net.ssl.trustStoreType": "JKS",
                                    "encrypted:javax.net.ssl.trustStorePassword": "<%= PasswordVault.get_password(@password_vault_name +'/'+ @asset_code, 'truststorepass' ) %>",
                                    "oracle.net.ssl_cipher_suites": "(TLS_RSA_WITH_AES_256_CBC_SHA256)",
									"oracle.net.ssl_version": "1.2",
                                    "javax.net.ssl.trustStore": "<%= @certs_location_path %>/<%= @common['trustStore'] %>",
                                    <% if @asset_code == 'obpsoa' %>
                                    "oracle.jdbc.ReadTimeout": "180000"
                                    <% else %>
                                    "oracle.jdbc.ReadTimeout": "300000"
                                    <% end %>
                                }
                            }
                        ]
                    }
                ]
            }
            <% end %>
