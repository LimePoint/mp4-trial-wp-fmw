                        "type": "CoherenceClusterParams",
                        "attributes": {
                          "SecurityFrameworkEnabled": "true"
                        }
                      }
                    ]
                  }
                ]
            },
            {
                "name": "DomainLogConfiguration",
                "type": "DomainLog",
                "attributes": {
                    "NumberOfFilesLimited": "true",
                    "FileMinSize": "10000",
                    "RotationTime": "05:00",
                    "FileTimeSpan": "24",
                    "RotateLogOnStartup": "true",
                    "RotationType": "byTime",
                    "FileCount": "5",
                    "FileName": "<%= @log_location_path %>/<%= @domain_name %>.log"
                }
            },
            {
                "name": "DomainSecurityConfiguration",
                "type": "SecurityConfiguration",
                "attributes": {
                  "ConnectionLoggerEnabled": "true",
                  "ConnectionFilter": "weblogic.security.net.ConnectionFilterImpl"
                },
                "params":
                [
                    {
                        "type": "Realm",
                        "name": "myrealm",
                        "attributes":
                        {
                            "DelegateMBeanAuthorization": "true"
                        },
                        "params": [
                          {
                            "type": "UserLockoutManager",
                            "attributes": {
                              "LockoutThreshold": "3",
                              "LockoutDuration": "15",
                              "LockoutResetDuration": "15"
                            }
                          },
                          {
                            "type": "PasswordValidator",
                            "name": "SystemPasswordValidator:com.bea.security.providers.authentication.passwordvalidator.SystemPasswordValidator",
                            "attributes": {
                              "MinPasswordLength": "15",
                              "MinUppercaseCharacters": "1",
                              "MinLowercaseCharacters": "1",
                              "MinNumericCharacters": "1",
                              "MinNonAlphanumericCharacters": "1"
                            }
                          }
                        ]
                    },
                    {
                        "type" : "CertRevoc",
                        "name" : "<%= @domain_name %>",
                        "attributes" : {
                          "CheckingEnabled" : "true"
                        }
                    }
                ]
            }
            ,
            <%= render "common-server-settings.erb",
