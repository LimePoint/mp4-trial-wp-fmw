<% environment_name = @environment['name'] %>
<% password_vault_name = @common['password_vault_name'] %>
<% dns_domain = @obpipm["dns_domain_name"] ; if dns_domain.nil? then dns_domain=".mintpress.io" end %>
<% asset_code = 'obpipm' %>

<% base_install_path = '/oracle/app/binaries/' + asset_code %>
<% base_domain_path  = '/oracle/app/runtime/' + asset_code + '/domains' %>
<% ucmVaultDirectory  = '/oracle/app/runtime/' + asset_code + '/' + environment_name %>

<% log_location_path   = '/oracle/app/logs/' + asset_code %>
<% certs_location_path = '/oracle/app/runtime/' + environment_name + '/certs' %>

<% oracle_install_path      = base_install_path + '/dbclient' %>
<% java_install_path      = base_install_path + '/java' %>
<% jdk_version            = '8' %>

<% fmw_install_path           = base_install_path + '/fmw' %>
<% inventory_install_path = base_install_path + '/oraInventory' %>
<% temp_location          = base_install_path + '/tmp' %>

<% ipm_admin_server_top   = base_domain_path %>
<% ipm_managed_server_top = '/oracle/app/binaries/runtime/' + asset_code + '/domains'  %>

<% node_manager_top       = fmw_install_path + '/wlserver/common/nodemanager' %>
<%
    if is_running_on_cloud
      ucm_connection_repository_secondary = @obpipm['frontend']['admin_host'].gsub(/-adm/, "01-prv") + ':' + @obpipm['ucm_server']['intradoc_port']
    else
      ucm_connection_repository_secondary = @obpipm['frontend']['admin_host'].gsub(/-adm/, "-e") + ':' + @obpipm['ucm_server']['intradoc_port']
    end
%>

<% case environment_name
when /svp.*r/
    auto_number_prefix = 'UCM_SVP2R-'
when /nft.*r/
    auto_number_prefix = 'UCM_NFT1R-'
when /prd.*r/
    auto_number_prefix = 'UCM_PRD2R-'
when /svp.*w/
    auto_number_prefix = 'UCM_SVP2W-'
when /nft.*w/
    auto_number_prefix = 'UCM_NFT1W-'
when /prd.*w/
    auto_number_prefix = 'UCM_PRD2W-'
else
    auto_number_prefix = "#{environment_name}-#{@obpipm['ucm_server']['cluster_name']}-"
end %>

